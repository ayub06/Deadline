<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blogspot.onayub.sqltrial" >
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.blogspot.onayub.sqltrial.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.blogspot.onayub.sqltrial.DisplayContact"
            android:label="Deadline">
            <intent-filter>
                <action android:name="com.blogspot.onayub.sqltrial.LAUNCH"/>
            </intent-filter>
        </activity>

    </application>
</manifest>